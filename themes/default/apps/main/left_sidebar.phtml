<!-- Include the Google Translate Element script -->
<script type="text/javascript">
    function googleTranslateElementInit() {
        new google.translate.TranslateElement({
            pageLanguage: 'en',
            layout: google.translate.TranslateElement.InlineLayout.SIMPLE
        }, 'google_translate_element');
    }

    function szChange(value) {
        if (value == 'pp' || value == 'PP') {
            return true;
        }
        window.location.href = 'https://p-tweets.com/talk/?cat_id=' + value;

    }

    $(document).ready(function () {
        $('#profile-svg').on('click', function (event) {
            event.preventDefault(); // Prevent default anchor behavior
            $('#sidebarProfileDropdownMenu').toggle(); // Toggle visibility
        });

        $('#more-svg').on('click', function (event) {
            event.preventDefault(); // Prevent default anchor behavior
            $('#mobileSideMoreDropDown').toggle(); // Toggle visibility
        });

        // Optional: Hide the dropdown when clicking outside
        $(document).on('click', function (event) {
            if (!$(event.target).closest('#profile-svg, #sidebarProfileDropdownMenu').length) {
                $('#sidebarProfileDropdownMenu').hide();
            }

            if (!$(event.target).closest('#more-svg, #mobileSideMoreDropDown').length) {
                $('#mobileSideMoreDropDown').hide();
            }
        });
        
          $('#sidebarProfileDropdownMenu a').on('click', function() {
             $('#sidebarProfileDropdownMenu').hide();
        });
        
        $('#mobileSideMoreDropDown a').on('click', function() {
             $('#mobileSideMoreDropDown').hide();
        });
    });


</script>
